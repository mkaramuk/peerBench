{
  "name": "@peerbench/cli",
  "version": "1.0.0",
  "description": "CLI tool for peerBench",
  "keywords": [
    "ai",
    "evaluation",
    "validation",
    "benchmark"
  ],
  "homepage": "https://github.com/Robust-AI-Bench/peerBench#readme",
  "bugs": {
    "url": "https://github.com/Robust-AI-Bench/peerBench/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Robust-AI-Bench/peerBench.git"
  },
  "license": "MIT",
  "author": "Forest AI Development Team",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "_lint": "tsc && eslint",
    "dev": "tsx src/index.ts",
    "_watch": "tsup --silent --watch --onSuccess 'tsc-alias -p tsconfig.json -f && node dist/index.js'",
    "_start": "node dist/index.js",
    "_build": "tsup --onSuccess 'tsc-alias -p tsconfig.json -f'",
    "_clean": "rm -rf dist",
    "_clean:outputs": "rm -rf data/output/*",
    "_clean:logs": "rm -rf data/logs/*"
  },
  "dependencies": {
    "@peerbench/sdk": "*",
    "@dotenvx/dotenvx": "^1.39.1",
    "@inquirer/prompts": "^7.4.1",
    "ansis": "^3.17.0",
    "axios": "^1.8.4",
    "commander": "^13.1.0",
    "csv": "^6.3.11",
    "glob": "^11.0.2",
    "hyparquet": "^1.11.0",
    "multiformats": "^13.3.2",
    "openai": "^4.92.1",
    "table": "^6.9.0",
    "uuid": "^11.1.0",
    "viem": "^2.26.2",
    "winston": "^3.17.0",
    "yaml": "^2.7.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@types/node": "^22.14.0",
    "eslint": "^9.24.0",
    "tsc-alias": "^1.8.13",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.0"
  }
}
