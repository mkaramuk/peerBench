{
  "name": "@peerbench/sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for peerBench",
  "keywords": [
    "ai",
    "evaluation",
    "validation",
    "benchmark"
  ],
  "homepage": "https://github.com/Robust-AI-Bench/peerBench",
  "bugs": {
    "url": "https://github.com/Robust-AI-Bench/peerBench/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Robust-AI-Bench/peerBench.git"
  },
  "license": "MIT",
  "author": "peerBench Development Team",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
