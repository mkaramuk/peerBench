    "commander": "^13.1.0",
    "csv": "^6.3.11",
    "fast-xml-parser": "^5.2.5",
    "glob": "^11.0.2",
    "hyparquet": "^1.11.0",
    "multiformats": "^13.3.2",
    "openai": "^4.92.1",
    "table": "^6.9.0",
    "uuid": "^11.1.0",
